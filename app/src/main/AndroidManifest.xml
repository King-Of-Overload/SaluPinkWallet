<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zjut.alan.pinkwallet">

    <application
        android:name=".PinkWalletApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.common.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".module.home.HomeActivity" />
        <activity android:name=".module.common.LoginActivity" />
        <activity android:name=".module.common.RegisterActivity" />
        <activity android:name=".module.common.ForgetPwdActivity"></activity>
    </application>

</manifest>